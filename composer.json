{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "algolia/algoliasearch-client-php": "^2.7",
        "anhskohbo/no-captcha": "^3.3",
        "behat/behat": "^3.8",
        "consoletvs/charts": "7.*",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel-notification-channels/discord": "^1.3",
        "laravel-notification-channels/facebook": "^0.2.1",
        "laravel-notification-channels/telegram": "^0.5.1",
        "laravel-notification-channels/twilio": "^3.1",
        "laravel-notification-channels/twitter": "^5.1",
        "laravel/cashier": "^12.6",
        "laravel/framework": "^8.12",
        "laravel/horizon": "^5.6",
        "laravel/scout": "^8.5",
        "laravel/telescope": "^4.4",
        "laravel/tinker": "^2.5",
        "lorisleiva/laravel-actions": "^2.0",
        "phpmd/phpmd": "^2.9",
        "spatie/crawler": "^6.0",
        "spatie/dns": "^1.6",
        "spatie/image-optimizer": "^1.3",
        "spatie/laravel-flash": "^1.8",
        "spatie/laravel-permission": "^3.18",
        "spatie/laravel-queueable-action": "^2.9",
        "spatie/laravel-searchable": "^1.9",
        "spatie/laravel-tags": "^3.0",
        "spatie/laravel-view-models": "^1.3",
        "spatie/once": "^2.2",
        "squirephp/airlines-en": "^2.0",
        "squirephp/airports-en": "^2.0",
        "squirephp/continents-en": "^2.0",
        "squirephp/currencies-en": "^2.0",
        "squirephp/gb-counties-en": "^2.0",
        "squirephp/regions-en": "^2.0",
        "squizlabs/php_codesniffer": "^3.5",
        "themsaid/laravel-mail-preview": "^4.0",
        "ycs77/laravel-form-builder-bs4": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.9",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/dusk": "^6.11",
        "laravel/envoy": "^2.5",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpbench/phpbench": "^0.17.1",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
